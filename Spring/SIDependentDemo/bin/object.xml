<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans  
               http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
               
               
         <bean id="stdObj" class="com.techment.model.Student">
         <property name="name" value="Namrata Sahu"></property>
         <property name="rollNo" value="301202217023" ></property>
         <property name="enrollment" value="BD2043"></property>
         <property name="department" ref="dept"></property>
         </bean>  
           
         
         <bean id="dept" class="com.techment.model.Department">
         <property name="deptName" value="B.Tech"></property>
         <property name="branch" value="CSE"></property>
         <property name="semester" value="8" ></property>
         
         </bean>        
   </beans>            